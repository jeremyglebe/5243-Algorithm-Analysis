{
    "name": "ls-for-psualg",
    "displayName": "Language Support for PsuAlg",
    "description": "Syntax highlighting for the pseudocode introduced in \"Introduction to Algorithms; Cormen, Leiserson, Rivest (2nd ed.)\"",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "psualg",
            "aliases": ["PsuAlg", "psualg"],
            "extensions": [".pseudo",".psu"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "psualg",
            "scopeName": "source.pseudo",
            "path": "./syntaxes/psualg.tmLanguage.json"
        }]
    }
}